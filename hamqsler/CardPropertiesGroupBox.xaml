<?xml version="1.0" encoding="utf-8"?>
<GroupBox
	x:Class="hamqsler.CardPropertiesGroupBox" 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	xmlns:local="clr-namespace:hamqsler">
	<StackPanel>
		<local:PrinterPropertiesGroupBox
			x:Name="printerPropsGroupBox"
			Header="Default Printer Properties"
			local:PaperSizePropertyChanged="printerPropertiesGroupBox1_PaperSizePropertyChanged" />
		<local:PrintPropertiesGroupBox
			x:Name="printPropsComboBox"
			Header="Default Print Properties"
			local:PrintPropertiesChanged="printPropertiesGroupBox1_PrintPropertiesChanged" />
		<local:CardAlignmentGroupBox
			x:Name="cardAlignmentGroupBox"
			Header="Default Card Alignment" />
	</StackPanel>
</GroupBox>