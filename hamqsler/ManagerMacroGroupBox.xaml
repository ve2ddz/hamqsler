<local:MacroGroupBox x:Class="hamqsler.ManagerMacroGroupBox"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:hamqsler"
	HeaderText="Manager Macro">
	<StackPanel>
		<local:MacroGroupBox x:Name="DesignTextGroupBox" Header="Design Text" Margin="20,1,1,1">
			<StackPanel x:Name="DesignTextPanel"/>
		</local:MacroGroupBox>
		<local:MacroGroupBox x:Name="NoManagerTextGroupBox" Header="No Manager Text" Margin="20,1,1,1">
			<StackPanel x:Name="NoManagerTextPanel"/>
		</local:MacroGroupBox>
	</StackPanel>
</local:MacroGroupBox>